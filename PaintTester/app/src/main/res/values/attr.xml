<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomRadioButton">
        <attr name="color" format="color"/>
    </declare-styleable>

    <declare-styleable name="PaintWidget">
        <attr name="maxWidth" format="integer"/>
        <attr name="defaultColorPosition" format="integer"/>
        <attr name="firstItemColor" format="enum">
            <enum name="JADE" value="0xFF00A86B"/>
            <enum name="NAVY" value="0xFF000080"/>
            <enum name="ANTHRACITE" value="0xFF464451"/>
        </attr>
    </declare-styleable>
</resources>